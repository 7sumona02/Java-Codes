import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
        
        int[] numbers = new int[5];
        numbers[0] = 1;
        numbers[1] = 2;
        numbers[2] = 3;
        numbers[3] = 4;
        numbers[4] = 5;
        
        Scanner scanner = new Scanner(System.in);
        System.out.println("which index to access?");
        int i = scanner.nextInt();
        
        System.out.println(numbers[i]);
    }
}
