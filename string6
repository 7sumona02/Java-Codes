import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("enter text: ");
        String text = scanner.nextLine();
        
        String[] pieces = text.split(" ");
        if(pieces.length>0){
            System.out.println(pieces[0]);
        }
    }
}
